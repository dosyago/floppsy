{
  "name": "floppsy",
  "version": "1.1.6",
  "description": "Hash designed for floating point hardware. 32, 64 or 128 bit digests.",
  "main": "node.js",
  "module": "index.js",
  "unpkg": "index.js",
  "browser": "dist/index.js",
  "browserslist": [
    "> 5%"
  ],
  "pre-commit": [
    "build"
  ],
  "scripts": {
    "test": "node -r esm index.js && node -r esm test.js",
    "build": "parcel build index.js --no-source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crislin2046/floppsy.git"
  },
  "keywords": [
    "floppsy",
    "floating point",
    "hash",
    "smhasher",
    "dieharder",
    "practrand",
    "string",
    "integer",
    "avalanche"
  ],
  "author": "@dosy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crislin2046/floppsy/issues"
  },
  "homepage": "https://github.com/crislin2046/floppsy#readme",
  "dependencies": {
    "esm": "^3.2.25",
    "utf8str": "^1.0.2"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2"
  }
}
